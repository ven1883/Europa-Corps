				<Sound file="Content/Items/Medical/Syringe.ogg" range="400" />
				
				<Affliction identifier="incision" amount="10" />
			</StatusEffect>

			<!-- Remove the item when fully used -->
			<StatusEffect type="OnBroken" target="This">
				<Remove />
			</StatusEffect>

		</MeleeWeapon>

	</Item>

